{
  "name": "chip",
  "icon": "media/icon.png",
  "author": "FutureAtoms",
  "version": "1.3.28",
  "repository": {
    "type": "git",
    "url": "https://github.com/FutureAtoms/chip"
  },
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "bugs": {
    "url": "https://github.com/FutureAtoms/chip/issues",
    "email": "support@futureatoms.com"
  },
  "homepage": "https://futureatoms.com/chipos.html",
  "qna": "https://github.com/FutureAtoms/chip/issues/new/choose",
  "license": "Apache-2.0",
  "displayName": "Chip - ChipOS AI Assistant",
  "pricing": "Free",
  "description": "The AI assistant for ChipOS",
  "publisher": "futureatoms",
  "engines": {
    "vscode": "^1.70.0",
    "node": ">=20.19.0"
  },
  "engine-strict": true,
  "galleryBanner": {
    "color": "#1E1E1E",
    "theme": "dark"
  },
  "categories": [
    "AI",
    "Chat",
    "Programming Languages",
    "Education",
    "Machine Learning",
    "Snippets"
  ],
  "keywords": [
    "chatgpt",
    "cline",
    "roo",
    "github",
    "copilot",
    "claude",
    "mistral",
    "codestral",
    "ai"
  ],
  "extensionDependencies": [
    "futureatoms.chipos-welcome"
  ],
  "activationEvents": [
    "onUri",
    "onStartupFinished",
    "onView:chipGUIView"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "filenames": [
          "config.json",
          ".chiprc.json"
        ],
        "id": "jsonc"
      },
      {
        "id": "promptLanguage",
        "extensions": [
          ".prompt"
        ],
        "aliases": [
          "Prompt Language"
        ],
        "configuration": "./prompt-file-language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "promptLanguage",
        "scopeName": "source.prompt",
        "path": "./media/prompt.tmLanguage.json"
      }
    ],
    "configuration": {
      "title": "Chip",
      "properties": {
        "chip.telemetryEnabled": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Chip collects anonymous usage data, cleaned of PII, to help us improve the product for our users. Read more  at [futureatoms.com › Telemetry](https://docs.futureatoms.com/telemetry).",
          "markdownDeprecationMessage": "This setting is deprecated and will be removed in a later version. Please use the _Telemetry > Allow Anonymous Telemetry_ setting in the extension's User Settings page.",
          "deprecationMessage": "Please use the _Telemetry > Allow Anonymous Telemetry_ setting in the extension's User Settings page."
        },
        "chip.showInlineTip": {
          "type": "boolean",
          "default": true,
          "description": "Show inline suggestion to use the Chip keyboard shortcuts (e.g. \"Cmd/Ctrl L to select code, Cmd/Ctrl I to edit\")."
        },
        "chip.disableQuickFix": {
          "type": "boolean",
          "default": false,
          "description": "Disable the quick fix feature."
        },
        "chip.enableQuickActions": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "Enable the experimental Quick Actions feature. Read our walkthrough to learn about configuration and how to share feedback: [futureatoms.com › Walkthrough: Quick Actions (experimental)](https://docs.futureatoms.com/customize/deep-dives/vscode-actions#quick-actions)"
        },
        "chip.enableTabAutocomplete": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Enable Chip's tab autocomplete feature. Read our walkthrough to learn about configuration and how to share feedback: [futureatoms.com › Walkthrough: Tab Autocomplete (beta)](https://docs.futureatoms.com/features/tab-autocomplete)"
        },
        "chip.enableNextEdit": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Enable Chip's next edit feature. Read our docs to learn about configuration and how to share feedback: [futureatoms.com › Features › Autocomplete › Next Edit (experimental)](https://docs.futureatoms.com/features/autocomplete/next-edit)"
        },
        "chip.pauseTabAutocompleteOnBattery": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "Pause Chip's tab autocomplete feature when your battery is low."
        },
        "chip.pauseCodebaseIndexOnStart": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "Pause Chip's codebase index on start."
        },
        "chip.enableConsole": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "Enable a console to log and explore model inputs and outputs. It can be found in the bottom panel."
        },
        "chip.remoteConfigServerUrl": {
          "type": "string",
          "default": null,
          "markdownDescription": "If your team is set up to use shared configuration, enter the server URL here and your user token below to enable automatic syncing."
        },
        "chip.userToken": {
          "type": "string",
          "default": null,
          "markdownDescription": "If your team is set up to use shared configuration, enter your user token here and your server URL above to enable automatic syncing."
        },
        "chip.remoteConfigSyncPeriod": {
          "type": "number",
          "default": 60,
          "description": "The period of time in minutes between automatic syncs."
        }
      }
    },
    "commands": [
      {
        "command": "chip.applyCodeFromChat",
        "category": "Chip",
        "title": "Apply code from chat",
        "group": "Chip"
      },
      {
        "command": "chip.acceptDiff",
        "category": "Chip",
        "title": "Accept Diff",
        "group": "Chip"
      },
      {
        "command": "chip.rejectDiff",
        "category": "Chip",
        "title": "Reject Diff",
        "group": "Chip",
        "icon": "$(stop)"
      },
      {
        "command": "chip.acceptVerticalDiffBlock",
        "category": "Chip",
        "title": "Accept Vertical Diff Block",
        "group": "Chip"
      },
      {
        "command": "chip.rejectVerticalDiffBlock",
        "category": "Chip",
        "title": "Reject Vertical Diff Block",
        "group": "Chip"
      },
      {
        "command": "chip.focusEdit",
        "category": "Chip",
        "title": "Add to Edit",
        "group": "Chip"
      },
      {
        "command": "chip.focusChipInput",
        "category": "Chip",
        "title": "Add Highlighted Code to Context and Clear Chat",
        "group": "Chip"
      },
      {
        "command": "chip.focusChipInputWithoutClear",
        "category": "Chip",
        "title": "Add to Chat",
        "group": "Chip"
      },
      {
        "command": "chip.debugTerminal",
        "category": "Chip",
        "title": "Debug Terminal",
        "group": "Chip"
      },
      {
        "command": "chip.exitEditMode",
        "category": "Chip",
        "title": "Exit Edit Mode",
        "group": "Chip"
      },
      {
        "command": "chip.openConfigPage",
        "category": "Chip",
        "title": "Open Settings",
        "icon": "$(gear)",
        "group": "Chip"
      },
      {
        "command": "chip.toggleTabAutocompleteEnabled",
        "category": "Chip",
        "title": "Toggle Autocomplete Enabled",
        "group": "Chip"
      },
      {
        "command": "chip.forceAutocomplete",
        "title": "Chip: Force Autocomplete",
        "category": "Chip"
      },
      {
        "command": "chip.selectFilesAsContext",
        "category": "Chip",
        "title": "Select Files as Context",
        "group": "Chip"
      },
      {
        "command": "chip.newSession",
        "category": "Chip",
        "title": "New Session",
        "icon": "$(add)",
        "group": "Chip"
      },
      {
        "command": "chip.shareSession",
        "category": "Chip",
        "title": "Share Current Chat Session as Markdown",
        "group": "Chip"
      },
      {
        "command": "chip.viewHistory",
        "category": "Chip",
        "title": "View History",
        "icon": "$(history)",
        "group": "Chip"
      },
      {
        "command": "chip.viewLogs",
        "category": "Chip",
        "title": "View Logs",
        "group": "Chip"
      },
      {
        "command": "chip.clearConsole",
        "category": "Chip",
        "title": "Clear Console",
        "icon": "$(clear-all)",
        "group": "Chip"
      },
      {
        "command": "chip.navigateTo",
        "category": "Chip",
        "title": "Navigate to a path",
        "group": "Chip"
      },
      {
        "command": "chip.writeCommentsForCode",
        "category": "Chip",
        "title": "Write Comments for this Code",
        "group": "Chip"
      },
      {
        "command": "chip.writeDocstringForCode",
        "category": "Chip",
        "title": "Write a Docstring for this Code",
        "group": "Chip"
      },
      {
        "command": "chip.fixCode",
        "category": "Chip",
        "title": "Fix this Code",
        "group": "Chip"
      },
      {
        "command": "chip.optimizeCode",
        "category": "Chip",
        "title": "Optimize this Code",
        "group": "Chip"
      },
      {
        "command": "chip.fixGrammar",
        "category": "Chip",
        "title": "Fix Grammar / Spelling",
        "group": "Chip"
      },
      {
        "command": "chip.codebaseForceReIndex",
        "category": "Chip",
        "title": "Codebase Force Re-Index",
        "group": "Chip"
      },
      {
        "command": "chip.rebuildCodebaseIndex",
        "category": "Chip",
        "title": "Rebuild codebase index",
        "group": "Chip"
      },
      {
        "command": "chip.docsIndex",
        "category": "Chip",
        "title": "Docs Index",
        "group": "Chip"
      },
      {
        "command": "chip.docsReIndex",
        "category": "Chip",
        "title": "Docs Force Re-Index",
        "group": "Chip"
      },
      {
        "command": "chip.focusChipInput",
        "category": "Chip",
        "title": "Focus Chip Chat",
        "group": "Chip"
      },
      {
        "command": "chip.enterEnterpriseLicenseKey",
        "category": "Chip",
        "title": "Enter Enterprise License Key",
        "group": "Chip"
      },
      {
        "command": "chip.toggleNextEditEnabled",
        "title": "Chip: Toggle Next Edit"
      },
      {
        "command": "chip.nextEditWindow.hideNextEditSuggestion",
        "category": "Chip",
        "title": "Hide Next Edit Suggestion"
      },
      {
        "command": "chip.nextEditWindow.acceptNextEditSuggestion",
        "category": "Chip",
        "title": "Accept Next Edit Suggestion"
      },
      {
        "command": "chip.forceNextEdit",
        "category": "Chip",
        "title": "Chip: Force Next Edit"
      },
      {
        "command": "chip.acceptJump",
        "category": "Chip",
        "title": "Chip: Accept Jump Suggestion"
      },
      {
        "command": "chip.rejectJump",
        "category": "Chip",
        "title": "Chip: Reject Jump Suggestion"
      },
      {
        "command": "chip.generateRule",
        "category": "Chip",
        "title": "Generate Rule",
        "group": "Chip"
      },
      {
        "command": "chip.openInNewWindow",
        "category": "Chip",
        "title": "Open in new window",
        "group": "Chip"
      },
      {
        "command": "chip.resetViewLocation",
        "category": "Chip",
        "title": "Reset View to Secondary Sidebar",
        "group": "Chip"
      }
    ],
    "keybindings": [
      {
        "command": "chip.focusChipInput",
        "mac": "cmd+l",
        "key": "ctrl+l"
      },
      {
        "command": "chip.focusChipInputWithoutClear",
        "mac": "cmd+shift+l",
        "key": "ctrl+shift+l"
      },
      {
        "command": "chip.acceptDiff",
        "mac": "shift+cmd+enter",
        "key": "shift+ctrl+enter",
        "when": "chip.diffVisible"
      },
      {
        "command": "chip.rejectDiff",
        "mac": "shift+cmd+backspace",
        "key": "shift+ctrl+backspace",
        "when": "chip.diffVisible"
      },
      {
        "command": "chip.rejectDiff",
        "mac": "cmd+z",
        "key": "ctrl+z",
        "when": "chip.diffVisible"
      },
      {
        "command": "chip.quickEditHistoryUp",
        "mac": "up",
        "key": "up",
        "when": "false && chip.quickEditHistoryFocused"
      },
      {
        "command": "chip.quickEditHistoryDown",
        "mac": "down",
        "key": "down",
        "when": "false && chip.quickEditHistoryFocused"
      },
      {
        "command": "chip.acceptVerticalDiffBlock",
        "mac": "alt+cmd+y",
        "key": "alt+ctrl+y"
      },
      {
        "command": "chip.rejectVerticalDiffBlock",
        "mac": "alt+cmd+n",
        "key": "alt+ctrl+n"
      },
      {
        "command": "chip.focusEdit",
        "title": "Edit code with natural language",
        "mac": "cmd+i",
        "key": "ctrl+i"
      },
      {
        "command": "chip.exitEditMode",
        "mac": "escape",
        "key": "escape",
        "when": "chip.inEditMode && editorFocus"
      },
      {
        "command": "chip.debugTerminal",
        "mac": "cmd+shift+r",
        "key": "ctrl+shift+r"
      },
      {
        "command": "chip.openInNewWindow",
        "mac": "cmd+k cmd+m",
        "key": "ctrl+k ctrl+m",
        "when": "!terminalFocus"
      },
      {
        "command": "chip.toggleTabAutocompleteEnabled",
        "mac": "cmd+k cmd+a",
        "key": "ctrl+k ctrl+a",
        "when": "!terminalFocus"
      },
      {
        "command": "chip.forceAutocomplete",
        "key": "ctrl+alt+space",
        "mac": "cmd+alt+space",
        "when": "editorTextFocus && !editorHasSelection && !editorReadOnly && !inSnippetMode"
      },
      {
        "command": "chip.applyCodeFromChat",
        "mac": "alt+a",
        "key": "alt+a"
      },
      {
        "command": "chip.toggleNextEditEnabled",
        "key": "ctrl+k ctrl+n",
        "mac": "cmd+k cmd+n",
        "when": "editorTextFocus"
      },
      {
        "command": "chip.nextEditWindow.hideNextEditSuggestion",
        "key": "escape",
        "when": "editorTextFocus && nextEditWindowActive && !chip.jumpDecorationVisible"
      },
      {
        "command": "chip.nextEditWindow.acceptNextEditSuggestion",
        "key": "tab",
        "when": "editorTextFocus && nextEditWindowActive && !chip.jumpDecorationVisible"
      },
      {
        "command": "chip.acceptJump",
        "key": "tab",
        "when": "editorTextFocus && chip.jumpDecorationVisible"
      },
      {
        "command": "chip.rejectJump",
        "key": "escape",
        "when": "editorTextFocus && chip.jumpDecorationVisible"
      }
    ],
    "submenus": [
      {
        "id": "chip.chipSubMenu",
        "label": "Chip"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "chip.focusChipInput"
        },
        {
          "command": "chip.focusChipInputWithoutClear"
        },
        {
          "command": "chip.debugTerminal"
        },
        {
          "command": "chip.newSession"
        },
        {
          "command": "chip.enterEnterpriseLicenseKey"
        },
        {
          "command": "chip.generateRule"
        },
        {
          "command": "chip.openInNewWindow"
        },
        {
          "command": "chip.resetViewLocation"
        }
      ],
      "editor/context": [
        {
          "submenu": "chip.chipSubMenu",
          "group": "0_achip"
        }
      ],
      "editor/title/run": [
        {
          "command": "chip.rejectDiff",
          "group": "Chip",
          "when": "chip.streamingDiff"
        }
      ],
      "chip.chipSubMenu": [
        {
          "command": "chip.focusChipInputWithoutClear",
          "group": "Chip"
        },
        {
          "command": "chip.focusEdit",
          "group": "Chip",
          "when": "!editorReadonly"
        }
      ],
      "explorer/context": [
        {
          "command": "chip.selectFilesAsContext",
          "group": "1_debug@1"
        }
      ],
      "view/title": [
        {
          "command": "chip.newSession",
          "group": "navigation@1",
          "when": "view == chip.chipGUIView"
        },
        {
          "command": "chip.viewHistory",
          "group": "navigation@2",
          "when": "view == chip.chipGUIView"
        },
        {
          "command": "chip.openConfigPage",
          "group": "navigation@4",
          "when": "view == chip.chipGUIView"
        },
        {
          "command": "chip.clearConsole",
          "group": "navigation@1",
          "when": "view == chip.chipConsoleView"
        }
      ],
      "editor/title": [
        {
          "command": "chip.newSession",
          "group": "navigation@1",
          "when": "activeWebviewPanelId == chip.chipGUIView"
        },
        {
          "command": "chip.viewHistory",
          "group": "navigation@2",
          "when": "activeWebviewPanelId == chip.chipGUIView"
        }
      ],
      "terminal/context": [
        {
          "command": "chip.debugTerminal",
          "group": "navigation@top"
        }
      ]
    },
    "viewsContainers": {
      "auxiliarybar": [
        {
          "id": "chip",
          "title": "Chip",
          "icon": "media/chip-activity.svg"
        }
      ],
      "panel": [
        {
          "id": "chipConsole",
          "title": "Chip Console",
          "icon": "$(window)"
        }
      ]
    },
    "views": {
      "chip": [
        {
          "type": "webview",
          "id": "chip.chipGUIView",
          "name": "Chip",
          "icon": "media/chip-activity.svg",
          "visibility": "visible"
        }
      ],
      "chipConsole": [
        {
          "type": "webview",
          "id": "chip.chipConsoleView",
          "name": "Chip Console",
          "icon": "$(window)",
          "visibility": "visible",
          "when": "config.chip.enableConsole"
        }
      ]
    },
    "jsonValidation": [
      {
        "fileMatch": "**/.chip*/config.json",
        "url": "./config_schema.json"
      },
      {
        "fileMatch": ".chiprc.json",
        "url": "./chip_rc_schema.json"
      },
      {
        "fileMatch": "**/config.yaml",
        "url": "./config-yaml-schema.json"
      }
    ]
  },
  "scripts": {
    "esbuild-base": "node scripts/esbuild.js",
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "esbuild-notify": "npm run esbuild-base -- --sourcemap --notify",
    "esbuild:visualize": "esbuild-visualizer --metadata ./build/meta.json --filename ./build/stats.html --open",
    "tsc": "tsc -p ./",
    "tsc:check": "tsc -p ./ --noEmit",
    "tsc-watch": "tsc -watch -p ./",
    "rebuild": "electron-rebuild -v 19.1.8 node-pty",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint . --ext ts --fix",
    "test": "vitest run",
    "vitest": "vitest run",
    "write-build-timestamp": "node scripts/write-build-timestamp.js",
    "prepackage": "node scripts/prepackage.js",
    "package": "node scripts/package.js",
    "package-all": "node scripts/package-all.js",
    "package:pre-release": "node scripts/package.js --pre-release",
    "build:rust": "cargo-cp-artifact -ac sync sync.node -- cargo build --manifest-path ../../sync/Cargo.toml --message-format=json-render-diagnostics",
    "build-debug:rust": "npm run build:rust --",
    "build-release:rust": "npm run build:rust -- --release",
    "e2e:compile": "tsc -p ./tsconfig.e2e.json",
    "e2e:build": "npm --prefix ../../gui run build && npm run package",
    "e2e:create-storage": "mkdir -p ./e2e/storage",
    "e2e:get-chromedriver": "extest get-chromedriver --storage ./e2e/storage --code_version 1.95.0",
    "e2e:get-vscode": "extest get-vscode --storage ./e2e/storage --code_version 1.95.0",
    "e2e:sign-vscode": "codesign --entitlements entitlements.plist --deep --force -s - './e2e/storage/Visual Studio Code.app'",
    "e2e:copy-vsix": "chmod +x ./e2e/get-latest-vsix.sh && bash ./e2e/get-latest-vsix.sh",
    "e2e:install-vsix": "extest install-vsix -f ./e2e/vsix/chip.vsix --extensions_dir ./e2e/.test-extensions --storage ./e2e/storage",
    "e2e:install-extensions": "extest install-from-marketplace ms-vscode-remote.remote-ssh --extensions_dir ./e2e/.test-extensions --storage ./e2e/storage && extest install-from-marketplace ms-vscode-remote.remote-containers --extensions_dir ./e2e/.test-extensions --storage ./e2e/storage && extest install-from-marketplace ms-vscode-remote.remote-wsl --extensions_dir ./e2e/.test-extensions --storage ./e2e/storage",
    "e2e:test": "NODE_ENV=e2e extest run-tests ${TEST_FILE:-'./e2e/_output/tests/*.test.js'} --code_settings settings.json --extensions_dir ./e2e/.test-extensions --storage ./e2e/storage",
    "e2e:clean": "rm -rf ./e2e/_output ./e2e/storage",
    "e2e:all": "npm run e2e:build && npm run e2e:compile && npm run e2e:create-storage && npm run e2e:get-chromedriver && npm run e2e:get-vscode && npm run e2e:sign-vscode && npm run e2e:copy-vsix && npm run e2e:install-vsix && npm run e2e:install-extensions && CHIP_GLOBAL_DIR=e2e/test-chip npm run e2e:test && npm run e2e:clean",
    "e2e:all-non-mac": "npm run e2e:build && npm run e2e:compile && npm run e2e:create-storage && npm run e2e:get-chromedriver && npm run e2e:get-vscode && npm run e2e:copy-vsix && npm run e2e:install-vsix && npm run e2e:install-extensions && CHIP_GLOBAL_DIR=e2e/test-chip npm run e2e:test && npm run e2e:clean",
    "e2e:recompile-extension": "npm run package && npm run e2e:compile && npm run e2e:copy-vsix && npm run e2e:install-vsix && npm run e2e:install-extensions && CHIP_GLOBAL_DIR=e2e/test-chip npm run e2e:test && npm run e2e:clean",
    "e2e:rebuild-gui": "rm -rf gui && cp -r ../../gui/dist gui && npm run package && npm run e2e:copy-vsix && npm run e2e:install-vsix && npm run e2e:install-extensions && CHIP_GLOBAL_DIR=e2e/test-chip npm run e2e:test && npm run e2e:clean",
    "e2e:quick": "npm run e2e:compile && CHIP_GLOBAL_DIR=e2e/test-chip npm run e2e:test && npm run e2e:clean",
    "e2e:ci:download": "npm run e2e:create-storage && npm run e2e:get-chromedriver && npm run e2e:get-vscode",
    "e2e:ci:run": "npm run e2e:compile && npm run e2e:copy-vsix && npm run e2e:install-vsix && npm run e2e:install-extensions && CHIP_GLOBAL_DIR=e2e/test-chip npm run e2e:test",
    "e2e:ci:run-yaml": "npm run e2e:compile && npm run e2e:copy-vsix && npm run e2e:install-vsix && npm run e2e:install-extensions && CHIP_GLOBAL_DIR=e2e/test-chip-yaml npm run e2e:test"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.4",
    "@nestjs/common": "^11.0.16",
    "@openapitools/openapi-generator-cli": "^2.23.1",
    "@types/chai": "^5.0.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/follow-redirects": "^1.14.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "16.x",
    "@types/node-fetch": "^2.6.12",
    "@types/react-dom": "^18.2.4",
    "@types/request": "^2.48.8",
    "@types/uuid": "^10.0.0",
